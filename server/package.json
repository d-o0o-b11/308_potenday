    "test": "NODE_ENV=test jest --config ./jest.unit.js",
    "test:watch": "jest --watch",
    "test:cov": "NODE_ENV=test jest test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./jest.e2e.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "seed:run": "source scripts/seed.run.sh ",
    "seed-read:run": "source scripts/seed-read.run.sh ",
    "seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create",
    "migration-read:run": "sh ./scripts/migration-read.run.sh",
    "migration:run": "sh ./scripts/migration.run.sh",
    "migration-read:generate": "sh ./scripts/migration-read.generate.sh",
    "migration:generate": "sh ./scripts/migration.generate.sh"
  },
